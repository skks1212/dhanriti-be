{
    "version": 2,
    "builds": [
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "maxLambdaSize": "15mb",
                "distDir": "staticfiles"
            }
        },
        {
            "src": "core/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "core/wsgi.py"
        }
    ],
    "crons": [
        {
            "path": "/v4/cron/3z1OxJQuzkvKKw9ZJDWnZE8R8SuysN7ghdFzwk870UzLXB84ww",
            "schedule": "*/1 * * * *"
        }
    ]
}